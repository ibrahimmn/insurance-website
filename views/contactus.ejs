         <div class="form-container">
            <form class="contact-form">
                <h2>Send us an email</h2>
                <label for="name">Name:</label><input type="text" name="name" id="name" placeholder="Full name"><br>
                <label for="name">Email:</label><input type="email" name="email" id="email" placeholder="example@gmail.com" required><br>
                <label for="name">Subject:</label><input type="text" name="subject" id="subject" placeholder="Subject"><br>
                <textarea id="message" placeholder="Message" required></textarea>
                <input type="submit" class="submit" value="Send">
    
            </form>
    
    
        </div>
    </div>
</div>


